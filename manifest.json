{
  "manifest_version": 3,
  "name": "Scoutbook Advancement Helper",
  "version": "1.0",
  "description": "Allows users to quickly submit all requirements for a rank to Scoutbook.",
  "permissions": ["scripting", "tabs"],
  "host_permissions": ["https://scoutbook.scouting.org/*"],
  "action": {
    "default_title": "Scoutbook Advancement Helper"
  },
  "background": {
    "service_worker": "background.js"
  }
}
